<div class="card m-3" style="max-width: 18rem;">
    <NavLink class="nav-link text-decoration-none" href="@Gacha.GachaId">
        <img src="img/gacha/@(Gacha.GachaId).png" class="card-img-top" alt="@Gacha.Name">
        <div class="card-footer text-white">
            @Gacha.Name
        </div>
    </NavLink>
</div>

@code {
    [Parameter]
    [EditorRequired]
    public Gacha Gacha { get; set; }
}
